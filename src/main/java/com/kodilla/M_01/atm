interface ATM {
	
	void moneyTradingOut(int payOut);
	
	void moneyTradingIn(int payIn);
	
	default void atmConnection() {
			System.out.println("Podaj kod PIN");
		}
	
	static String atmEnd() {
			return "Transakcja zakonczona";
		}
}
	
class ATMImpl implements ATM {
	
	public void moneyTradingOut(int payOut) {
			System.out.println("Wyplac: " + payOut);
		}
	
	public void moneyTradingIn(int payIn) {
			System.out.println("Wplac: " + payIn);
		}
		
}
		
class Application{	
	
   	public static void main (String[] args) throws java.lang.Exception {
       
    	ATMImpl atmImpl = new ATMImpl();

		atmImpl.atmConnection();
		
		atmImpl.moneyTradingIn(1000);

		atmImpl.moneyTradingOut(500);
		
		System.out.println(ATM.atmEnd());
			
   }
}
